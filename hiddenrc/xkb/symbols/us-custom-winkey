default partial alphanumeric_keys
xkb_symbols "basic" {
	include "us"
	name[Group1] = "English (US, international with custom ctrl>alt esc>caps)";
  key <LCTL> { [Alt_L, Meta_L] };
  key <LALT> { [Control_L, Control_L] };
	key <CAPS> { [Escape, Hyper_L] };
  key <LWIN> { [Escape, Super_L] };
  modifier_map Mod3 { <CAPS> };
  modifier_map Mod3 { <HYPR> };
};
